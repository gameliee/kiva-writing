  author = {Mollick, Ethan},
  date = {2014-01},
  journaltitle = {Journal of Business Venturing},
  shortjournal = {211},
  volume = {29},
  number = {1},
  pages = {1--16},
  issn = {08839026},
  doi = {10.1016/j.jbusvent.2013.06.005},
  url = {https://linkinghub.elsevier.com/retrieve/pii/S088390261300058X},
  urldate = {2023-08-18},
  abstract = {Crowdfunding allows founders of for-profit, artistic, and cultural ventures to fund their efforts by drawing on relatively small contributions from a relatively large number of individuals using the internet, without standard financial intermediaries. Drawing on a dataset of over 48,500 projects with combined funding over \$237 M, this paper offers a description of the underlying dynamics of success and failure among crowdfunded ventures. It suggests that personal networks and underlying project quality are associated with the success of crowdfunding efforts, and that geography is related to both the type of projects proposed and successful fundraising. Finally, I find that the vast majority of founders seem to fulfill their obligations to funders, but that over 75\% deliver products later than expected, with the degree of delay predicted by the level and amount of funding a project receives. These results offer insight into the emerging phenomenon of crowdfunding, and also shed light more generally on the ways that the actions of founders may affect their ability to receive entrepreneurial financing.},
  langid = {english},
  keywords = {master thesis,Q1 Business and International Management,Q1 Management of Technology and Innovation},
  annotation = {2230 citations (Crossref) [2023-08-19]},
  file = {/Users/dat/Applications Data/Zotero/storage/XJ9IY5C3/Mollick - 2014 - The dynamics of crowdfunding An exploratory study.pdf}
}

