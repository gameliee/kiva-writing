{
    "latex-workshop.latex.recipes": [
        {
            "name": "pdflatex -> bibtex -> pdflatex * 2",
            "tools": [
                "pdflatex",
                "makeglossaries",
                "bibtex",
                "pdflatex",
                "pdflatex"
            ]
        }
    ],
    "latex-workshop.latex.tools": [
        {
            "name": "pdflatex",
            "command": "pdflatex",
            "args": [
                "-synctex=1",
                "-interaction=nonstopmode",
                "-file-line-error",
                "%DOC%"
            ],
            "env": {}
        },
        {
            "name": "bibtex",
            "command": "bibtex",
            "args": [
                "%DOCFILE%"
            ],
            "env": {}
        },
        {
            "name": "makeglossaries",
            "command": "makeglossaries",
            "args": [
                "%DOCFILE%"
            ],
            "env": {}
        }
    ],
    "pasteImage.basePath": "${projectRoot}",
    "pasteImage.showFilePathConfirmInputBox": true,
    "pasteImage.prefix": "\\begin{figure}[H]\n\t\\centering\n\t\\includegraphics[width=0.8\\textwidth]{",
    "pasteImage.suffix": "}\n\t\\caption{}\n\t\\label{fig:${imageFileNameWithoutExt}}\n\\end{figure}",
    "pasteImage.filePathConfirmInputBoxMode": "onlyName",
    "pasteImage.path": "${projectRoot}/images",
    "plantuml.exportSubFolder": false,
    "plantuml.exportOutDir": "images"
}